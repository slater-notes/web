    "@testing-library/user-event": "^13.1.1",
    "@types/jest": "^26.0.22",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "source-map-explorer": "~2.5.2",
    "standard-version": "^9.1.1",
    "web-vitals": "^1.1.0"
  }
