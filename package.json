    "@types/styled-components": "^5.1.9",
    "async": "^3.2.0",
    "axios": "~0.21.1",
    "bluebird": "^3.7.2",
    "easy-peasy": "^4.0.1",
    "formik": "^2.2.6",
    "is-hotkey": "~0.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nanoid": "^3.1.16",
    "react": "^17.0.2",
    "react-dom": "^17.0.1",
    "react-feather": "^2.0.9",
    "react-scripts": "^4.0.1",
    "slate": "~0.59.0",
    "slate-history": "~0.59.0",
    "slate-react": "~0.59.0",
    "styled-components": "^5.2.1",
